generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Occurrence {
  id              Int       @id @default(autoincrement()) @map("id")
  createTime     DateTime? @default(now()) @db.DateTime(0) @map("create_time")
  updateTime     DateTime? @default(now()) @db.DateTime(0) @map("update_time")
  petName        String?   @db.VarChar(50) @map("pet_name")
  author          String?   @db.VarChar(50) @map("author")
  lat             Float?    @db.Float @map("lat")
  lon             Float?    @db.Float @map("lon")
  petDescription String?   @db.Text @map("pet_description")
  isAuthorsPet  Boolean? @map("is_authors_pet")
  solved          Boolean? @map("solved")

  @@map("occurrences")
}